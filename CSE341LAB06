.MODEL SMALL
 
.STACK 100H

.DATA 
ARR DW ?
M1 DB 0AH,0DH,'INPUT:$'
M2 DB 0AH,0DH,'OUTPUT: $'
M3 DW 0AH,0DH,"EQUAL$"
M4 DW 0AH,0DH,"NOT EQUAL$"
.CODE
;TASK 01:Input a number. Find all the factors of that number and store them in stack. 
MAIN PROC
MOV AX,@DATA
MOV DS,AX

LEA DX,M1
MOV AH,9
INT 21H

MOV AH,1
INT 21H

PUSH AX



;TASK 02:Input a string and reverse it using stack.
MAIN PROC
MOV AX,@DATA
MOV DS,AX

LEA DX,M1
MOV AH,9
INT 21H

MOV CL,0
FUN:
MOV AH,1
INT 21H
CMP AL,0DH
JE END_
     
PUSH AX
INC CL
JMP FUN
END_:
MOV AH,9
LEA DX,M2
INT 21H
 
FUN2:
CMP CL,0
JE END_IF
DEC CL
MOV AH,2
POP DX
INT 21H
JMP FUN2
END_IF:






    
    
    
    
    
